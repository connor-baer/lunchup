    "express": "^4.15.3",
    "fs": "0.0.1-security",
    "lodash": "^4.17.4",
    "lowdb": "^0.16.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "request": "^2.81.0",
    "twig": "^1.10.5",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
