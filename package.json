{
  "name": "lunchup",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connor-baer/lunchup.git"
  },
  "author": "Connor Baer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connor-baer/lunchup/issues"
  },
  "homepage": "https://github.com/connor-baer/lunchup#readme",
  "scripts": {
    "build": "npm run prebuild && NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rm -rf ./dist",
    "fix": "npm run fix:prettier; npm run fix:eslint",
    "fix:eslint": "eslint --fix --quiet \"server/**/*.js\"",
    "fix:prettier": "prettier --config prettier.config.js --write \"server/**/*.js\"",
    "prebuild": "npm run clean",
    "start": "npm run prebuild && webpack --config webpack.config.js --display-error-details",
    "precommit": "lint-staged",
    "test": "npm run test:lint --silent && npm run test:unit --silent",
    "test:lint": "eslint --cache --quiet server/**/*.js",
    "test:lint:watch": "onchange \"**/*.js\" -- npm run test:lint-js",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "lint-staged": {
    "*.js": [
      "npm run fix:prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@slack/client": "^3.14.2",
    "ajv": "^5.3.0",
    "async": "^2.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "bufferutil": "^3.0.3",
    "cron": "^1.3.0",
    "cryptr": "^2.0.0",
    "ejs": "^2.5.6",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "ladda-cache": "^0.1.4",
    "ladda-logger": "^0.2.1",
    "lodash": "^4.17.4",
    "lowdb": "^0.16.2",
    "moment": "^2.19.2",
    "mongodb": "^2.2.33",
    "monk": "^6.0.5",
    "morgan": "^1.9.0",
    "request": "^2.83.0",
    "start-server-webpack-plugin": "^2.2.0",
    "twig": "^1.10.5",
    "utf-8-validate": "^3.0.4",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-node-externals": "^1.6.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.13.4",
    "jest": "^21.2.1",
    "lint-staged": "^3.6.0",
    "prettier": "^1.8.2"
  }
}
