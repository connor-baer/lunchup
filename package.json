  "scripts": {
    "fix": "npm run fix:prettier; npm run fix:eslint; npm run fix:stylelint",
    "fix:eslint": "eslint --fix --quiet \"**/*.js\" \"**/*.js\"",
    "fix:prettier": "prettier --single-quote --write \"**/*.js\" \"**/*.js\"",
    "fix:stylelint":
      "stylelint \"src/**/*.scss\" --syntax scss --config stylelint.config.js --fix",
    "start": "nodemon --inspect app.js --ignore 'data/*'",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": ["npm run fix:prettier", "git add"],
    "*.scss": ["npm run fix:stylelint", "git add"]
  },
  "dependencies": {
    "@slack/client": "^3.10.0",
