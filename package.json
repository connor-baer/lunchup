  "scripts": {
    "build":
      "npm run prebuild && NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rm -rf ./dist",
    "fix": "npm run fix:prettier; npm run fix:eslint",
    "fix:eslint": "eslint --fix --quiet \"**/*.js\" \"**/*.js\"",
    "fix:prettier": "prettier --single-quote --write \"**/*.js\" \"**/*.js\"",
    "prebuild": "npm run clean",
    "start":
      "npm run prebuild && webpack --config webpack.config.js --display-error-details",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": ["npm run fix:prettier", "git add"]
  },
  "dependencies": {
